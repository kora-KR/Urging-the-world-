page{
  background: #f5f5f5
}
view, text{line-height: 1}
/* main */
.main{
  padding: 10rpx 30rpx 0;
  box-sizing: border-box;
  font-size: 26rpx;
  color: #666;
  background: #fff
}
.mainTop{
  padding-top: 30rpx;
}
.main>view:not(:nth-last-of-type(1)){
  border-bottom: 1rpx solid #e5e5e5
}
.name{
  margin: 14rpx 0 24rpx;
  font-size: 32rpx;
  color: #333;
  font-weight: 600;
  line-height: 44rpx;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap
}
.state{
  padding: 26rpx 0 6rpx;
}
.state>view{
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  margin-bottom: 12rpx;
  text-align: right
}
.state>view>.qk{
  text-align: left
}
.state>view>text{
  display: block;
}
.state>view>text:nth-of-type(1){
  width: 180rpx
}
.proState{
  line-height: 100rpx
}
.state>view>text:nth-of-type(2){
  color: #222
}
.proState{
  font-size: 30rpx;
  color: #666
}
.relationTip{
  font-size: 24rpx;
  color: #999
}
.proStateOve{
  font-weight: 600
}

.relation{
  margin: 40rpx auto;
  width: 690rpx;
  line-height: 88rpx;
  text-align: center;
  background: #fbfafc;
  border: 1rpx solid #d9d9e0;
  border-radius: 10rpx;
  font-size: 32rpx;
  color: #222
}


/* zzc */
.zzc{
  position: fixed;
  left: 0;
  top: 0;
  z-index: 999;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, .5)
}
.zzcTip{
  position: absolute;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  width: 70%;
  line-height: 80rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 8rpx;
  text-align: center;
  font-size: 28rpx;
  color: #5458ec;
  font-weight: 600
}
.zzcTip>view{
  border-bottom: 1rpx solid #f2f2f2;
  display: flex;
  justify-content: center;
  align-items: center;
  line-height: 80rpx;
}
.zzcTip image{
  display: inline-block;
  margin-right: 24rpx;
  width: 32rpx;
  height: 32rpx;
}
.allTip{
  line-height: 104rpx;
  font-size: 32rpx;
  color: #333;
}
.other-button-hover {
  background-color: #eee;
}
.button_rel::after{
  border: none
}
.lh{
  line-height: 34rpx
}