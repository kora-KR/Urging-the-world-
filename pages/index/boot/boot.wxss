/* 引导页 */
.boot{
  position: fixed;
  top: 0;
  z-index: 999;
  height: 100%;
  width: 100%;
  background: #fff
}
.headerImg{
  display: block;
  margin-bottom: 40rpx;
  width: 100%;
  height: 488rpx;
}
.open{
  width: 690rpx;
  height: 96rpx;
  border-radius: 8rpx;
  font-size: 36rpx;
  color: #fff
}
button::after{
  border: 1rpx solid #d6d5d6
}
.bootWarn{
  position: fixed;
  top: 0;
  z-index: 999;
  padding-top: 80rpx;
  box-sizing: border-box;
  height: 100%;
  width: 100%;
  background: #fff;
  font-size: 32rpx;
  color: #222;
  text-align: center;
}
.bootWarn>image{
  display: block;
  margin: 0 auto 48rpx;
  width: 168rpx;
  height: 168rpx;
}
.bootWarn>view:nth-of-type(1){
  margin-bottom: 20rpx;
}
.openAgain{
  position: absolute;
  bottom: 90rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 690rpx;
  line-height: 96rpx;
  font-size: 34rpx;
  box-shadow: 0rpx 4rpx 6rpx -2rpx rgba(134, 142, 255, .6);
}
.openAgain::after{
  border: 0
}
.button-hover {
  background: #900
}
.relation_item{
  line-height: 80rpx
}
.navigator-hover{
  background: #eeeef3
l}
/* btn */
.btn{
  margin-bottom: 30rpx;
  width: 680rpx;
  box-shadow: 0rpx 4rpx 6rpx -2rpx rgba(134, 142, 255, .6);
  font-size: 34rpx;
  line-height: 96rpx;
  height: 96rpx;
  background: #5458ec;
  color: #fff
}
.other-button-hover {
  background-color: #900;
}