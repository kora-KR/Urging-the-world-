page{
background: #f3f3f8
}
view, text{
  line-height: 1
}
.my{
  position: fixed;
  top: 0;
  z-index: 999;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 0 30rpx;
  width: 100%;
  height: 150rpx;
  background: #3e2865;
  color: #fff
}
.my image{
  margin-right: 50rpx;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%
}
/* main */
.item{
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 40rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 1rpx solid #eeeef3;
  background: #fff
}
.item>view:nth-of-type(1){
  margin-right: 24rpx;
  width: 88rpx;
  line-height: 88rpx;
  border: 1rpx solid #9aa3e8;
  border-radius: 50%;
  font-size: 24rpx;
  text-align: center;
  font-weight: 600
}
.item>view:nth-of-type(2){
  flex: 1
}
.compony_name{
  width: 545rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 30rpx;
}
.allTip{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #666
}
.time{
  font-size: 24rpx;
  color: #999
}
.item_right{
  height: 88rpx;
}
.navigator-hover{
  background: #eeeef3
}
.city{
  display: inline-block;
  margin-right: 30rpx;
  max-width: 200rpx;
  overflow:hidden;
  text-overflow:ellipsis;
  white-space:nowrap;
}
.my_tip{
  position: fixed;
  z-index: 888;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  font-size: 30rpx;
  background: #fff;
  text-align: center;
  color: #9297A0
}
.my_tip>image{
  margin: 280rpx auto 30rpx;
  width: 318rpx;
  height: 234rpx
}
.phcolor{
  color: #999
}
.user_name{
  margin-bottom: 8rpx
}

.load_tip{
  position: fixed;
  z-index: 999;
  width: 100%;
  height: 100%;
  background: #fff;
  text-align: center;
  font-size: 36rpx;
}
.load_tip>view:nth-of-type(1){
  font-weight: 600;
  color: #222
}
.load_tip>view:nth-of-type(2){
  margin: 112rpx auto 58rpx;
  font-weight: 400;
  font-size: 28rpx;
  color: #9297a0
}
.load_tip>image{
  margin: 112rpx auto 58rpx;
  width: 144rpx;
  height: 144rpx
}