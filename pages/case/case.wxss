page {
  height: 100%;
  background: #f3f3f8;
}
view, text{
  line-height: 1
}
image{
  display: block
}

.header_img{
  width: 100%;
  height: 224rpx
}
.main{
  padding: 30rpx 20rpx;
  box-sizing: border-box;
}
.main>.case_item{
  position: relative;
  display: flex;
  justify-content: flex-start;
  align-items: center;
  padding: 32rpx;
  box-sizing: border-box;
  background: #fff;
  border-radius: 10rpx;
  box-shadow: 0rpx 3rpx 10rpx 6rpx rgba(166, 180, 255, .2)
}
.case_item>.case_icon{
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  right: 0rpx;
  width: 10rpx;
  height: 20rpx
}
.main>.case_item:not(:nth-last-of-type(1)){
  margin-bottom: 20rpx
}
.case_item image{
  margin-right: 26rpx;
  width: 72rpx;
  height: 72rpx
}
.case_text>view:nth-of-type(1){
  margin-bottom: 20rpx;
  font-size: 32rpx;
  font-weight: 600;
  color: #222
}
.case_text>view:nth-of-type(2){
  font-size: 26rpx;
  color: #717087
}
.navigator-hover{
  background: #900
}