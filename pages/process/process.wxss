page {
  height: 100%;
  background: #f3f3f8;
}
view, text{
  line-height: 1
}
image{
  display: block
}

.header_img{
  width: 100%;
  height: 224rpx
}
.main{
  position: absolute;
  top: 0;
  left: 0;
  z-index: -1;
  width: 100%;
  padding-top: 224rpx;
  box-sizing: border-box;
  height: 100%;
  background: #fff
}
.process_item{
  font-size: 36rpx;
  color: #222
}
.process_item>view{
  display: flex;
  justify-content: flex-start;
   align-items: center; 
  margin: 0 auto;
  width: 472rpx;
}
.process_item text{
  line-height: 48rpx;
}
.process_item:nth-of-type(1){
  padding-top: 66rpx;
}
.process_item>view>view>image{
  margin: 0 32rpx 0 6rpx;
  width: 88rpx;
  height: 88rpx;
}
.bg_int{
  margin: 20rpx auto 30rpx; 
  width: 472rpx;
  height: 24rpx;
}